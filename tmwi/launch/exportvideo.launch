<launch>

    <arg name="file"    default="/home/bagfiles/jesse/experiment_2023-07-05-16-00-48.bag"/>

    <node pkg="rosbag" type="play" name="rosbag" required="true" args="$(arg file)"/>

    <node name="image_decompr" pkg="image_transport" type="republish" args="compressed in:=camera/image_raw raw out:=camera_out/image" />

    <node name="extract" pkg="image_view" type="extract_images" respawn="false" required="true" output="screen" cwd="ROS_HOME">
        <remap from="image" to="/camera_out/image"/>
    </node>

</launch>